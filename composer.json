{
    "name": "laraxgram/core",
    "description": "The core of the LaraGram!",
    "keywords": ["framework", "laragram", "laravel", "laraquest", "bot", "telegram"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laraxgram/core/issues",
        "source": "https://github.com/laraxgram/core"
    },
    "authors": [
        {
            "name": "laraXgram",
            "email": "laraxgram@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laraxgram/laraquest": "^1",
        "laraxgram/tempora": "^1"
    },
    "replace": {
        "laraxgram/auth": "self.version",
        "laraxgram/bus": "self.version",
        "laraxgram/cache": "self.version",
        "laraxgram/collections": "self.version",
        "laraxgram/concurrency": "self.version",
        "laraxgram/conditionable": "self.version",
        "laraxgram/config": "self.version",
        "laraxgram/console": "self.version",
        "laraxgram/container": "self.version",
        "laraxgram/contracts": "self.version",
        "laraxgram/conecrsation": "self.version",
        "laraxgram/database": "self.version",
        "laraxgram/encryption": "self.version",
        "laraxgram/events": "self.version",
        "laraxgram/filesystem": "self.version",
        "laraxgram/hashing": "self.version",
        "laraxgram/jdb": "self.version",
        "laraxgram/keyboard": "self.version",
        "laraxgram/listening": "self.version",
        "laraxgram/log": "self.version",
        "laraxgram/macroable": "self.version",
        "laraxgram/pipeline": "self.version",
        "laraxgram/process": "self.version",
        "laraxgram/queue": "self.version",
        "laraxgram/redis": "self.version",
        "laraxgram/request": "self.version",
        "laraxgram/support": "self.version",
        "laraxgram/template": "self.version",
        "laraxgram/translation": "self.version",
        "laraxgram/validation": "self.version"
    },
    "autoload": {
        "files": [
            "src/Collections/functions.php",
            "src/Collections/helpers.php",
            "src/Events/functions.php",
            "src/Filesystem/functions.php",
            "src/Foundation/helpers.php",
            "src/Log/functions.php",
            "src/Support/functions.php",
            "src/Support/helpers.php",
            "src/Keyboard/helpers.php",
            "src/Request/helpers.php",
            "src/Console/Prompts/helpers.php",
            "src/Console/Prompts/Convertor/Functions.php"
        ],
        "psr-4": {
            "LaraGram\\": "src/",
            "LaraGram\\Support\\": [
                "src/Macroable/",
                "src/Collections/",
                "src/Conditionable/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "suggest": {
        "ext-apcu": "Required to use the APC cache driver.",
        "ext-fileinfo": "Required to use the Filesystem class.",
        "ext-memcached": "Required to use the memcache cache driver.",
        "ext-pcntl": "Required to use all features of the queue worker and console signal trapping.",
        "ext-pdo": "Required to use all database features.",
        "ext-posix": "Required to use all features of the queue worker.",
        "ext-redis": "Required to use the Redis cache and queue drivers (^4.0|^5.0|^6.0).",
        "ext-gmp": "*",
        "ext-bcmath": "*"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
